{
  "name": "Web application",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind",
  "workspaceFolder": "/workspace/projects/web/",
  "customizations": {
    "vscode": {
      "settings": {
        "git.openRepositoryInParentFolders": "always"
      },
      "extensions": [
        "ms-dotnettools.csdevkit"
      ]
    },
    "codespaces": {
      "openFiles": [
        "Program.cs"
      ]
    }
  }
}